{
  "$schema": "https://raw.githubusercontent.com/denoland/deno/main/cli/schemas/config-file.v1.json",
  "workspace": ["./auth", "./db", "./client", "./api", "./types"],
  "unstable": ["temporal", "kv"],
  "fmt": {
    "useTabs": false,
    "lineWidth": 120,
    "indentWidth": 2,
    "semiColons": true,
    "singleQuote": true
  },
  "imports": {
    "@da/bcrypt": "jsr:@da/bcrypt@^1.0.1",
    "@faker-js/faker": "npm:@faker-js/faker@^9.8.0",
    "@hono/zod-validator": "npm:@hono/zod-validator@^0.7.0",
    "@openauthjs/openauth": "npm:@openauthjs/openauth@^0.4.3",
    "@std/assert": "jsr:@std/assert@^1.0.13",
    "@std/fs": "jsr:@std/fs@^1.0.18",
    "@std/path": "jsr:@std/path@^1.1.0",
    "@std/random": "jsr:@std/random@^0.1.2",
    "@std/testing": "jsr:@std/testing@^1.0.14",
    "hono": "npm:hono@^4.7.11",
    "unstorage": "npm:unstorage@^1.16.0",
    "sqlite": "https://deno.land/x/sqlite/mod.ts",
    "zod": "npm:zod@^3.25.64",
    "denomailer": "https://deno.land/x/denomailer/mod.ts"
  }
}
