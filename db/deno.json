{
  "$schema": "https://raw.githubusercontent.com/denoland/deno/main/cli/schemas/config-file.v1.json",
  "name": "@collision-calendar/db",
  "version": "0.1.0",
  "tasks": {
    "db:generate": "deno run -A --env-file --node-modules-dir npm:drizzle-kit generate",
    "db:migrate": "deno run -A  --env-file --node-modules-dir npm:drizzle-kit migrate",
    "db:studio": "deno run -A --env-file --node-modules-dir npm:drizzle-kit studio"
  },
  "exports": {
    "./schema": "./schema/mod.ts",
    "./init": "./init/mod.ts",
    "./util": "./util/mod.ts"
  }
}
